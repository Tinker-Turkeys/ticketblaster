<h3 class="text-muted">Registrations:</h3>
<% @event.finalized_registrations.each do |registration| %>
  <% registration_data = registration.to_a(:arrays_to_strings => true) %>
  <div class="col-lg-6">
    <table class="table">
      <tr>
        <th>Label</th>
        <th>Response</th>
      </tr>
      <% registration_data.each do |cf| %>
        <tr>
          <td class="col-lg-3"><%= cf[0] %></td>
          <td><%= cf[1] %></td>
        </tr>
      <% end %>
    </table>
  </div>
<% end %>
